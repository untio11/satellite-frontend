{
  "name": "project-management-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "start-8080": "cross-env PORT='8080' react-app-rewired start",
    "build": "cross-env GENERATE_SOURCEMAP=false react-app-rewired build",
    "generate:TS": "graphql-codegen --config config.yml",
    "generate-with-key:TS": "node ./scripts/nodejs/generate_TS_from_devGQL.js",
    "generate-with-key:TS:LOCAL": "node ./scripts/nodejs/generate_TS_from_localGQL.js",
    "test": "react-app-rewired test",
    "test:ci": "react-app-rewired test --watchAll=false",
    "eject": "react-scripts eject",
    "lint:all": "prettier --check \"./src/**/*.{ts,tsx,js,json}\" && tslint --project ./tsconfig.json",
    "lint:js:fix": "prettier --write \"./src/**/*.{js,jsx,json}\"",
    "lint:less:fix": "stylelint \"./src/**/*.less\" --fix",
    "lint:ts:fix": "prettier --write \"./src/**/*.{ts,tsx}\" && tslint --fix --project ./tsconfig.json",
    "lint:all:fix": "prettier --write \"./src/**/*.{ts,tsx,js,json}\" && tslint --fix --project ./tsconfig.json",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,json}": [
      "yarn --silent lint:js:fix",
      "git add"
    ],
    "./src/**/*.{ts,tsx}": [
      "yarn --silent lint:ts:fix",
      "git add"
    ],
    "./src/**/*.{less}": [
      "yarn --silent lint:less:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hoc": "^3.1.3",
    "@cyclomedia/map-viewer-core": "^1.1.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/jszip": "^3.1.7",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-color": "^3.0.1",
    "@types/react-dnd": "^3.0.2",
    "@types/react-dom": "^16.9.0",
    "@types/shpjs": "^3.4.0",
    "antd": "^4.3.3",
    "apollo-boost": "^0.4.7",
    "connected-react-router": "^6.6.1",
    "cuid": "^2.1.8",
    "graphql": "^14.6.0",
    "history": "^4.10.1",
    "immer": "^6.0.1",
    "jszip": "^3.2.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "oidc-client": "^1.10.1",
    "ol": "^5.3.3",
    "proj4": "^2.6.0",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-color": "^2.18.0",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-oidc": "^4.0.0-beta1",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "shpjs": "^3.4.3",
    "three": "^0.121.0",
    "typesafe-actions": "latest",
    "typescript": "~3.7.2",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.12.2",
    "@graphql-codegen/introspection": "1.12.2",
    "@graphql-codegen/typescript": "1.12.2",
    "@graphql-codegen/typescript-operations": "1.12.2",
    "@graphql-codegen/typescript-react-apollo": "1.12.2",
    "@types/graphql": "^14.5.0",
    "@types/history": "^4.7.3",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.4",
    "@types/redux-thunk": "^2.1.0",
    "apollo": "^2.22.1",
    "babel-plugin-import": "^1.13.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^0.9.1",
    "husky": "^4.0.9",
    "js-yaml": "^3.14.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "node-fetch": "^2.6.1",
    "prettier": "^1.19.1",
    "redux-thunk": "^2.3.0",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0"
  }
}
